    <script>
        // Theme toggle functionality
        const themeToggle = document.getElementById('theme-toggle');
        const html = document.documentElement;

        // Check for saved theme preference or default to light mode
        const savedTheme = localStorage.getItem('theme');
        if (savedTheme) {
            html.classList.toggle('dark', savedTheme === 'dark');
        } else {
            // Check system preference
            const prefersDark = window.matchMedia('(prefers-color-scheme: dark)').matches;
            html.classList.toggle('dark', prefersDark);
        }

        themeToggle.addEventListener('click', () => {
            html.classList.toggle('dark');
            const isDark = html.classList.contains('dark');
            localStorage.setItem('theme', isDark ? 'dark' : 'light');
        });

        // Mobile sidebar functionality
        const mobileMenuButton = document.getElementById('mobile-menu-button');
        const mobileSidebar = document.getElementById('mobile-sidebar');
        const mobileSidebarBackdrop = document.getElementById('mobile-sidebar-backdrop');
        const mobileSidebarClose = document.getElementById('mobile-sidebar-close');

        function openMobileSidebar() {
            mobileSidebar.classList.remove('-translate-x-full');
            mobileSidebarBackdrop.classList.remove('hidden');
        }

        function closeMobileSidebar() {
            mobileSidebar.classList.add('-translate-x-full');
            mobileSidebarBackdrop.classList.add('hidden');
        }

        mobileMenuButton.addEventListener('click', openMobileSidebar);
        mobileSidebarClose.addEventListener('click', closeMobileSidebar);
        mobileSidebarBackdrop.addEventListener('click', closeMobileSidebar);

    </script>

    <!-- Main application JavaScript -->
    <script src="/js/app.js"></script>
    <script src="/js/dashboard.js"></script>
    <script>
        // Enhanced theme system for Tailwind v4
        function initTheme() {
            // Check system preference first, then localStorage
            const savedTheme = localStorage.getItem('theme');
            const systemPrefersDark = window.matchMedia('(prefers-color-scheme: dark)').matches;
            const initialTheme = savedTheme || (systemPrefersDark ? 'dark' : 'light');

            applyTheme(initialTheme);
        }

        function applyTheme(theme) {
            const html = document.documentElement;
            const themeButton = document.getElementById('theme-toggle');

            if (theme === 'dark') {
                html.classList.add('dark');
                localStorage.setItem('theme', 'dark');
            } else {
                html.classList.remove('dark');
                localStorage.setItem('theme', 'light');
            }

            // Update button appearance (icons and text will update via CSS)
            if (themeButton) {
                // Force a repaint to ensure CSS updates
                themeButton.style.opacity = '0.99';
                setTimeout(() => {
                    themeButton.style.opacity = '';
                }, 10);
            }

            // Dispatch custom event for other components
            window.dispatchEvent(new CustomEvent('themeChanged', {
                detail: { theme }
            }));
        }

        function toggleTheme() {
            const currentTheme = document.documentElement.classList.contains('dark') ? 'dark' : 'light';
            const newTheme = currentTheme === 'dark' ? 'light' : 'dark';
            applyTheme(newTheme);

            // Visual feedback
            const button = document.getElementById('theme-toggle');
            if (button) {
                button.style.transform = 'scale(0.95)';
                setTimeout(() => {
                    button.style.transform = '';
                }, 150);
            }
        }

        // Listen for system theme changes
        window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', (e) => {
            if (!localStorage.getItem('theme')) {
                applyTheme(e.matches ? 'dark' : 'light');
            }
        });

        // Initialize theme immediately
        initTheme();

        // Initialize the application
        document.addEventListener('DOMContentLoaded', () => {
            // Theme toggle handler
            document.getElementById('theme-toggle')?.addEventListener('click', toggleTheme);

            // Initialize FormAI app
            try {
                window.formaiApp = new FormaiApp();
            } catch (error) {
                console.log('FormAI app initialization failed, using basic navigation');

                // Basic navigation fallback
                document.querySelectorAll('.nav-item').forEach(item => {
                    item.addEventListener('click', (e) => {
                        e.preventDefault();
                        const page = item.dataset.page;

                        // Update nav state
                        document.querySelectorAll('.nav-item').forEach(nav => {
                            nav.classList.remove('bg-blue-100', 'dark:bg-blue-900/50', 'text-blue-700', 'dark:text-blue-200');
                            nav.classList.add('text-gray-700', 'dark:text-gray-300');
                        });

                        item.classList.remove('text-gray-700', 'dark:text-gray-300');
                        item.classList.add('bg-blue-100', 'dark:bg-blue-900/50', 'text-blue-700', 'dark:text-blue-200');

                        // Show/hide sections
                        document.querySelectorAll('.page-section').forEach(section => {
                            section.style.display = 'none';
                        });

                        const targetSection = document.getElementById(page + '-section');
                        if (targetSection) {
                            targetSection.style.display = 'block';
                        }

                        // Update page title
                        const titleElement = document.querySelector('header h1');
                        if (titleElement) {
                            titleElement.textContent = page.charAt(0).toUpperCase() + page.slice(1);
                        }
                    });
                });

                // Profile Modal Functionality
                const profileModal = document.getElementById('profile-modal');
                const addProfileBtn = document.getElementById('add-profile-btn');
                const addProfileCard = document.getElementById('add-profile-card');
                const closeProfileModal = document.getElementById('close-profile-modal');
                const cancelProfile = document.getElementById('cancel-profile');
                const profileForm = document.getElementById('profile-form');

                function openProfileModal() {
                    profileModal?.classList.remove('hidden');
                    document.body.style.overflow = 'hidden';
                }

                function closeProfileModalFn() {
                    profileModal?.classList.add('hidden');
                    document.body.style.overflow = 'auto';
                    profileForm?.reset();
                }

                addProfileBtn?.addEventListener('click', openProfileModal);
                addProfileCard?.addEventListener('click', openProfileModal);
                closeProfileModal?.addEventListener('click', closeProfileModalFn);
                cancelProfile?.addEventListener('click', closeProfileModalFn);

                profileModal?.addEventListener('click', (e) => {
                    if (e.target === profileModal) {
                        closeProfileModalFn();
                    }
                });

                profileForm?.addEventListener('submit', (e) => {
                    e.preventDefault();
                    const formData = new FormData(profileForm);
                    const profileData = Object.fromEntries(formData.entries());

                    if (!profileData.firstName || !profileData.lastName || !profileData.email || !profileData.phone) {
                        alert('Please fill in all required fields.');
                        return;
                    }

                    console.log('Profile data:', profileData);
                    alert('Profile saved successfully!');
                    closeProfileModalFn();
                });

                // URL Modal Functionality
                const urlModal = document.getElementById('url-modal');
                const addUrlBtn = document.getElementById('add-url-btn');
                const addUrlCard = document.getElementById('add-url-card');
                const closeUrlModal = document.getElementById('close-url-modal');
                const cancelUrl = document.getElementById('cancel-url');
                const urlForm = document.getElementById('url-form');
                const testUrlBtn = document.getElementById('test-url');

                function openUrlModal() {
                    urlModal?.classList.remove('hidden');
                    document.body.style.overflow = 'hidden';
                }

                function closeUrlModalFn() {
                    urlModal?.classList.add('hidden');
                    document.body.style.overflow = 'auto';
                    urlForm?.reset();
                }

                addUrlBtn?.addEventListener('click', openUrlModal);
                addUrlCard?.addEventListener('click', openUrlModal);
                closeUrlModal?.addEventListener('click', closeUrlModalFn);
                cancelUrl?.addEventListener('click', closeUrlModalFn);

                urlModal?.addEventListener('click', (e) => {
                    if (e.target === urlModal) {
                        closeUrlModalFn();
                    }
                });

                testUrlBtn?.addEventListener('click', () => {
                    const urlInput = document.getElementById('targetUrl');
                    const url = urlInput?.value;

                    if (!url) {
                        alert('Please enter a URL first.');
                        return;
                    }

                    try {
                        new URL(url);
                        alert('URL test initiated! Check the console for results.');
                        console.log('Testing URL:', url);
                    } catch (error) {
                        alert('Please enter a valid URL.');
                    }
                });

                urlForm?.addEventListener('submit', (e) => {
                    e.preventDefault();
                    const formData = new FormData(urlForm);
                    const urlData = Object.fromEntries(formData.entries());

                    if (!urlData.targetUrl || !urlData.urlName || !urlData.urlCategory) {
                        alert('Please fill in all required fields.');
                        return;
                    }

                    try {
                        new URL(urlData.targetUrl);
                    } catch (error) {
                        alert('Please enter a valid URL.');
                        return;
                    }

                    console.log('URL data:', urlData);
                    alert('URL saved successfully!');
                    closeUrlModalFn();
                });

                // Mappings functionality
                const mappingUrlSelect = document.getElementById('mapping-url-select');
                const scanFieldsBtn = document.getElementById('scan-fields-btn');
                const mappingInterface = document.getElementById('mapping-interface');
                const noMappingSelected = document.getElementById('no-mapping-selected');
                const mappingGrid = document.getElementById('mapping-grid');
                const scanStatus = document.getElementById('scan-status');

                mappingUrlSelect?.addEventListener('change', (e) => {
                    const hasSelection = e.target.value !== '';
                    if (scanFieldsBtn) {
                        scanFieldsBtn.disabled = !hasSelection;
                        if (hasSelection) {
                            scanFieldsBtn.classList.remove('opacity-50', 'cursor-not-allowed');
                        } else {
                            scanFieldsBtn.classList.add('opacity-50', 'cursor-not-allowed');
                        }
                    }
                });

                scanFieldsBtn?.addEventListener('click', () => {
                    if (!mappingUrlSelect?.value) return;

                    scanStatus?.classList.remove('hidden');
                    if (scanFieldsBtn) {
                        scanFieldsBtn.disabled = true;
                        scanFieldsBtn.innerHTML = '<i class="fas fa-spinner animate-spin mr-2"></i>Scanning...';
                    }

                    setTimeout(() => {
                        scanStatus?.classList.add('hidden');
                        if (scanFieldsBtn) {
                            scanFieldsBtn.disabled = false;
                            scanFieldsBtn.innerHTML = '<i class="fas fa-search mr-2"></i>Scan Form Fields';
                        }
                        noMappingSelected?.classList.add('hidden');
                        mappingGrid?.classList.remove('hidden');
                    }, 2000);
                });

                // Drag and drop functionality
                let draggedElement = null;

                document.addEventListener('dragstart', (e) => {
                    if (e.target.classList.contains('profile-field') || e.target.classList.contains('form-field')) {
                        draggedElement = e.target;
                        e.target.style.opacity = '0.5';
                    }
                });

                document.addEventListener('dragend', (e) => {
                    if (e.target.classList.contains('profile-field') || e.target.classList.contains('form-field')) {
                        e.target.style.opacity = '1';
                        draggedElement = null;
                    }
                });

                document.addEventListener('dragover', (e) => {
                    e.preventDefault();
                });

                document.addEventListener('drop', (e) => {
                    e.preventDefault();
                    if (draggedElement && (e.target.classList.contains('profile-field') || e.target.classList.contains('form-field'))) {
                        const sourceField = draggedElement.dataset.field;
                        const targetField = e.target.dataset.field;

                        if (sourceField && targetField && sourceField !== targetField) {
                            const activeMappings = document.getElementById('active-mappings');
                            const mappingElement = document.createElement('div');
                            mappingElement.className = 'flex items-center justify-between p-2 bg-white dark:bg-gray-800 rounded border';
                            mappingElement.innerHTML = `
                                <span class="text-sm text-gray-900 dark:text-white">${sourceField} â†’ ${targetField}</span>
                                <button class="text-red-500 hover:text-red-700 text-xs" onclick="this.parentElement.remove()">
                                    <i class="fas fa-times"></i>
                                </button>
                            `;
                            activeMappings?.appendChild(mappingElement);
                        }
                    }
                });

                // Quick Actions functionality
                document.addEventListener('click', (e) => {
                    if (e.target.closest('button')?.textContent?.includes('New Profile')) {
                        openProfileModal();
                    } else if (e.target.closest('button')?.textContent?.includes('Add URL')) {
                        openUrlModal();
                    }
                });
            }
        });
    </script>
</body>
</html>